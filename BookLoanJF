    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        
        card_no="";
        book_ID="";
        branch_ID="";
        card_no=jTextField1.getText();
        book_ID=jTextField2.getText();
        branch_ID=jTextField3.getText();
        
        if(card_no.isEmpty() && book_ID.isEmpty() && branch_ID.isEmpty())
        {
            JOptionPane.showMessageDialog(null,"Please enter the details");
        }
        else
        {
            if(card_no.length() !=4 || book_ID.length() !=10)
            {
                JOptionPane.showMessageDialog(null,"1. Card number must be 4 digits \n 2.Book ID must be 10 digits");
            }
            else
            {
                message=s.checkOUT(Integer.parseInt(card_no), book_ID, Integer.parseInt(branch_ID));
                JOptionPane.showMessageDialog(null,message);
            }
            
            
        }
        
        
    }//GEN-LAST:event_jButton1ActionPerformed

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        // TODO add your handling code here:
        String[][] res =new String[100][7];
        int c_no=0;
        book_ID="";
        card_no="";
        fname="";
        lname="";
        message="";
        
        book_ID=jTextField4.getText();
        card_no=jTextField5.getText();
        fname=jTextField6.getText();
        lname=jTextField7.getText();
        
        System.out.println("book_id"+book_ID);
        System.out.println("card_no"+card_no);
        System.out.println("fname"+fname);
        System.out.println("lname"+lname);
        
        if(!card_no.isEmpty())
        {
            c_no=Integer.parseInt(card_no);
        }if(card_no.isEmpty() && book_ID.isEmpty() && fname.isEmpty() && lname.isEmpty())
        {
            JOptionPane.showMessageDialog(null,"Please enter the mandatory details");
        }
        else
        
        {
          if(card_no.length() !=4 && card_no.length() !=0)
            {
                JOptionPane.showMessageDialog(null,"Card number must be 4 digits");
            }
          else if (book_ID.length()!=0 && book_ID.length()!=10 )
          {
              JOptionPane.showMessageDialog(null,"Card number must be 10 digits");
          }
            else
            {
                //message=s.checkOUT(Integer.parseInt(card_no), book_ID, Integer.parseInt(branch_ID));
               res=s.checkIN(c_no,book_ID,fname,lname);
               
                if(res[0][0]==null)
                {
                    jPanel2.setVisible(false);
                    JOptionPane.showMessageDialog(null,"No Book for given details");
                }
                else
                {
                    jPanel2.setVisible(true);
                    tableModel=new DefaultTableModel(res,columnName_checkout);
                    jTable1.setModel(tableModel);
                }
            }
            
            
        }
        
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        // TODO add your handling code here:
        jLabel26.setVisible(false);
            jLabel27.setVisible(false);
            jButton7.setVisible(false);
        
        float choice=s.checkIN_Book(l_id);
        //JOptionPane.showMessageDialog(null,message);
        jPanel2.setVisible(false);
        if(choice ==1 )
        {
            JOptionPane.showMessageDialog(null,"Checke IN successfully!!!!");
        }
        else if(choice ==0)
        {
            JOptionPane.showMessageDialog(null,"Checke IN  not successfully!!!!");
        }
        else if(choice > 1)
        {
            jLabel26.setVisible(true);
            jLabel27.setVisible(true);
            jLabel27.setText("$"+String.valueOf(choice));
            fine_amount=choice;
            jButton7.setVisible(true);
                
        }
        
        
    }//GEN-LAST:event_jButton3ActionPerformed

