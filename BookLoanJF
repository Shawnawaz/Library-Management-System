    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton8ActionPerformed
        // TODO add your handling code here:
        int loan_id=0;
        String lid;
        float amount=0;
        Fines fine=new Fines();
        lid=jTextField20.getText().toString();
        System.out.println(lid);
        
        
        if(lid.isEmpty())
        {
            JOptionPane.showMessageDialog(null,"Enter loan ID");
            
        }
        else
        {
            loan_id=Integer.parseInt(lid);
            System.out.println(loan_id);
            amount=fine.calculate_Fines(loan_id);
            if(amount == -2)
                    {
                        JOptionPane.showMessageDialog(null,"The Book is already checked IN!!!!");
                        
                    }
            else if(amount == -1)
            {
                jLabel29.setVisible(true);
                jLabel30.setVisible(true);
                jLabel30.setText("$"+"0");
            }
            else if(amount == -3)
            {
                JOptionPane.showMessageDialog(null,"In Valid Loan ID, Please enter valid Loan ID!!!!");
            }
            else
            {
                
                jLabel29.setVisible(true);
                jLabel30.setVisible(true);
                
                jLabel30.setText("$"+String.valueOf(amount));
            }
        }
        
        
        
        
    }//GEN-LAST:event_jButton8ActionPerformed
    
   
                
