    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        // TODO add your handling code here:
        jLabel16.setVisible(false);
        jLabel17.setVisible(false);
        fname="";
        lname="";
        address="";
        city="";
        state="";
        phone ="";
        fname=jTextField8.getText();
        lname=jTextField9.getText();
        address=jTextField10.getText();
        city=jTextField11.getText();
        state=jTextField12.getText();
        phone=jTextField13.getText();
        
        if(address.isEmpty() || city.isEmpty() || fname.isEmpty() || lname.isEmpty() || state.isEmpty() || phone.isEmpty())
        {
            JOptionPane.showMessageDialog(null,"Please enter all the details");
        }
        else
        {
            int result;
            result=s.addBorrower(fname,lname,address,city,state,phone);
            
            if(result==1)
            {
                jLabel16.setVisible(false);
                 jLabel17.setVisible(false);
               JOptionPane.showMessageDialog(null,"User Details already exist in DB"); 
            }
            else if(result > 1)
            {
                 jLabel16.setVisible(true);
                 jLabel17.setVisible(true);
                 jLabel17.setText(Integer.toString(result));
            }
        }
        
        
    }//GEN-LAST:event_jButton4ActionPerformed

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
        // TODO add your handling code here:
        
        String[][] res =new String[1000][8];
        book_ID ="";
        title="";
        author="";
        fname="";
        lname="";
        minit="";
        
        book_ID=jTextField14.getText();
        title=jTextField15.getText();
        author=jTextField16.getText();
        //Enable the fname, lname and minit fields based on the radio button.
        //Disable the author fields.
        if(jRadioButton1.isSelected())
        {
            
            author="";
            fname=jTextField17.getText();
            lname=jTextField18.getText();
            minit=jTextField19.getText();
        }
        
        if(!jRadioButton1.isSelected())
        {
            
            author=jTextField16.getText();
            fname="";
            lname="";
            minit="";
        }
        
        //logic
        
        if(book_ID.isEmpty() && title.isEmpty() && author.isEmpty() && fname.isEmpty() && lname.isEmpty() && minit.isEmpty())
        {
            JOptionPane.showMessageDialog(null,"Please enter book details");
        }  
        else
        {
            //To check the book_id is equal to 10 digits, else throw an error message without getting into the app.
            if(book_ID.length()==10 || book_ID.length()== 0)
            {
                //JOptionPane.showMessageDialog(null,"Book_ID must be 10 digits");
                res=s.search(book_ID,title,author,fname,lname,minit);
                
                //to check whether the result set is null
                if((res[0][0]==null))
                {
                    jPanel8.setVisible(false);
                    jLabel24.setVisible(true);
                    jLabel24.setText("Book does not exist!!!!");
            
                }
                else
                {
                    jLabel24.setVisible(false);
                    jPanel8.setVisible(true);
                    tableModel = new DefaultTableModel(res, columnName_search);
                    jTable2.setModel(tableModel);
                }  
        
            }
            else
        {
            JOptionPane.showMessageDialog(null,"Book_ID must be 10 digits");
            jTextField14.setText("");
        }
        }    
    }//GEN-LAST:event_jButton5ActionPerformed

    private void jRadioButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jRadioButton1ActionPerformed
        // TODO add your handling code here:
        
        if(jRadioButton1.isSelected())
        {
            jLabel20.setVisible(false);
            jTextField16.setVisible(false);
            jLabel21.setVisible(true);
            jLabel22.setVisible(true);
            jLabel23.setVisible(true);
            jTextField17.setVisible(true);
            jTextField18.setVisible(true);
            jTextField19.setVisible(true);
            
        }
         if(!jRadioButton1.isSelected())
         {
            jLabel20.setVisible(true);
            jTextField16.setVisible(true);
            jLabel21.setVisible(false);
            jLabel22.setVisible(false);
            jLabel23.setVisible(false);
            jTextField17.setVisible(false);
            jTextField18.setVisible(false);
            jTextField19.setVisible(false);
         }
    }//GEN-LAST:event_jRadioButton1ActionPerformed

